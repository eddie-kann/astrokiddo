{
  "name": "astrokiddo-web",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve astrokiddo-web --proxy-config proxy.conf.json",
    "build": "ng build astrokiddo-web --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
